{
  "name": "axpdsp-site",
  "version": "1.0.0",
  "description": "Start package for [Bulma](http://bulma.io)",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "dependencies": {
    "bulma": "^0.7.1",
    "bulma-calendar": "^5.0.1",
    "bulma-carousel": "^3.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^9.1.5",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "node-sass": "^4.9.3",
    "postcss-cli": "^6.0.0"
  },
  "scripts": {
    "css-build": "node-sass _sass/main.scss css/main.css",
    "css-deploy": "npm run css-build && npm run css-postcss",
    "css-postcss": "postcss --use autoprefixer --output css/main.css css/main.css",
    "css-watch": "npm run css-build -- --watch",
    "deploy": "npm run css-deploy && npm run js-build",
    "js-build": "babel _javascript --out-dir lib",
    "js-watch": "npm run js-build -- --watch",
    "start": "npm run css-watch | npm run js-watch",
    "postinstall": "cp node_modules/bulma-calendar/dist/js/bulma-calendar.min.js ./_javascript/bulma-calendar.min.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
